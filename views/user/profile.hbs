<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1>User Profile</h1>
        <hr>
        <h2>My Orders</h2>

        <div class="panel panel-default">
            <div class="panel-body">
                <ul class="list-group">
                    {{#each orders.items}}
                        <li class="list-group-item">
                            <span class="badge">{{this.price}} $
                            </span>
                            {{this.item.title}} | {{this.Qty}}
                        </li>
                    {{/each}}

                </ul>
            </div>
            <div class="panel-footer">
                <strong>Total Price : {{orders.cart.totalPrice }}</strong></div>
        </div>

    </div>
</div>